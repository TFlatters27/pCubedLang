FizzBuzz(100)

# This is comment
SUBROUTINE FizzBuzz(i)
  FOR a <- 1 TO i
    str <- ""
    IF (a MOD 3) = 0 THEN
      str <- str + "Fizz"
    ENDIF
    IF (a MOD 5) = 0 THEN
      str <- str + "Buzz"
    ENDIF
    IF ((a MOD 5) != 0 AND (a MOD 3) != 0) THEN
      str <- str + INT_TO_STRING(a)
    ENDIF
    OUTPUT str
  ENDFOR
ENDSUBROUTINE