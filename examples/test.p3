RECORD Wheel
   diameter : Integer | 10
   tread_depth : Real | 1.6
   manufacturer : String | "Unknown"
ENDRECORD

RECORD Car
  make : String | "undefined"
  model : String | "undefined"
  reg : String | "undefined"
  price : Real | "1000"
  noOfDoors : Integer | 5
  wheel_type : Wheel | "undefined"
ENDRECORD

