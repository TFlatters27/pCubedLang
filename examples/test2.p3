RECORD Wheel
  diameter : Integer | 10
  tread_depth : Real | 1.6
  manufacturer : String | "Unknown"
  testArray : Integer[] | [1,2,3]
ENDRECORD

RECORD Car
  make : String | "undefined"
  model : String | "undefined"
  reg : String
  price : Real | 10.00
  noOfDoors : Integer | 5
  testArray : Integer[] | [1,2,3]
  testMatrix : Char[][] | [['a'], ['b']]
ENDRECORD

carWheel <- Wheel(diameter: 16, tread_depth: 2.1, manufacturer: 'Goodyear')

OUTPUT carWheel # Wheel {diameter: 16, tread_depth: 2.1, manufacturer: "Goodyear"}

myCar <- Car('Ford', 'Focus', 'DX17 GYT', 1399.99, 5, [1,2,3,4], [['x'], ['y'], ['z']])

OUTPUT myCar

OUTPUT myCar.model

myVan <- Car(make: 'Mazda', reg: 'MJ67 KYU', noOfDoors: 3)

OUTPUT myVan

OUTPUT myVan.model

OUTPUT LEN(myVan.testMatrix)