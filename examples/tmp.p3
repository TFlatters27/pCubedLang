SUBROUTINE showAdd(a, b)
  result <- a + b
  OUTPUT result
ENDSUBROUTINE

SUBROUTINE sayHi()
  OUTPUT "Hi"
ENDSUBROUTINE

SUBROUTINE add(a, b)
  result <- a + b
  RETURN result
ENDSUBROUTINE

showAdd(2,3)
showAdd("Hello", "World!")

sayHi()

addition <- add(2,3) * 3
OUTPUT addition
concat <- add("Goodbye", "Universe!")
OUTPUT concat